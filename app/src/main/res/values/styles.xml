<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/text_color8</item>
        <item name="colorPrimaryDark">@color/rummy_mainColor</item>
        <item name="colorAccent">@color/text_color2</item>
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="colorControlNormal">@color/text_color2</item>
        <item name="android:statusBarColor">@color/rummy_mainColor</item>
        <item name="android:windowBackground">#000000</item>
        <item name="android:navigationBarColor">@color/black</item>
        <item name="cardViewStyle">@style/CardViewStyle</item>
    </style>

    <style name="addressVerificationDarkHome" parent="AppTheme">
        <item name="android:statusBarColor">@color/rummy_mainColor</item>
        <item name="android:navigationBarColor">@color/rummy_black_3</item>
    </style>

    <style name="addressVerificationLight" parent="AppTheme">
        <item name="android:statusBarColor">@color/rummy_statusBarColor</item>
        <item name="android:navigationBarColor">@color/rummy_windowBackColor</item>
        <item name="android:windowBackground">@color/rummy_windowBackColor</item>

    </style>

    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowMinWidthMinor">16dp</item>
    </style>

    <style name="CardViewStyle">
        <item name="cardBackgroundColor">@color/rummy_black_3</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme.Safe" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/rummy_colorPrimary</item>
        <item name="colorPrimaryDark">@color/rummy_colorPrimary</item>
        <item name="colorAccent">@color/rummy_colorPrimary</item>
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="colorControlNormal">@color/rummy_colorPrimary</item>
    </style>

    <style name="Theme.DialogActivity" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="AppTheme.Regular" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/app_purple</item>
        <item name="colorPrimaryDark">@color/app_purple</item>
        <item name="colorAccent">@color/app_purple</item>
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="android:splitMotionEvents">false</item>
        <item name="android:windowEnableSplitTouch">false</item>
        <item name="colorControlNormal">@color/app_purple</item>
    </style>

    <style name="Theme.Transprent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="Theme.AppUpdateTheme" parent="AppTheme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="Theme.Youtube.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="ProgressBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/rummy_progressTintColor</item>
    </style>

    <style name="SplashAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowBackground">@drawable/img_onboarding_rummy_bg</item>
    </style>

    <style name="AppTheme.Onboard" parent="Theme.AppCompat.Light.NoActionBar">
        <!--        <item name="android:windowAnimationStyle">@null</item>-->
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">@color/rummy_navigationColor</item>

    </style>

    <style name="AppTheme.FullScreen" parent="Theme.AppCompat.Light.NoActionBar">
        <!--        <item name="android:windowAnimationStyle">@null</item>-->
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTypeSwitch" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/rummy_colorPrimary</item>
    </style>

    <style name="AppTypeSwitch.Blue" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/rummy_colorPrimary</item>
    </style>

    <style name="CropImage.Theme.Transprent" parent="Theme.AppCompat">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>
    //Buttons

    <style name="Widget.MyTeam11.RaisedButton" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/raised_button_blue</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:paddingLeft">48dp</item>
        <item name="android:paddingRight">48dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    //Buttons

    <style name="Widget.MyTeam11.RaisedButton.Small" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/raised_button_blue</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Widget.MyTeam11.ParentBottomButton" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/view_shadow</item>
        <item name="android:gravity">center</item>RE
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:textColor">@color/cool_grey</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.MyTeam11.BorderEditText" parent="Widget.AppCompat.EditText">
        <item name="android:background">@drawable/rounded_rect_border_gray</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/cool_grey</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:padding">@dimen/margin_min</item>
        <item name="android:drawablePadding">@dimen/margin_small</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Widget.MyTeam11.BoxEditText" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:textSize">12sp</item>
        <item name="android:colorAccent">@color/rummy_azure_two</item>
    </style>


    <style name="Widget.MyTeam11.BoxEditTextV2" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/rubik_bold</item>
        <item name="android:colorAccent">@color/text_color8</item>
        <item name="hintTextColor">@color/text_color2</item>
        <item name="android:textColorHint">@color/text_color2</item>
    </style>
    <style name="TextLabel" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/rubik_regular</item>
    </style>

    <style name="Widget.MyTeam11.BorderEditTextV2" parent="Widget.AppCompat.EditText">
        <item name="android:textSize">16sp</item>
        <item name="backgroundTint">@color/text_color5</item>
        <item name="android:textColor">@color/gray1</item>
        <item name="android:textColorHint">@color/text_color2</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="android:drawablePadding">@dimen/margin_small</item>
        <item name="android:gravity">center_vertical</item>
    </style>


    <style name="Widget.MyTeam11.TextInputLayout" parent="AppTheme.Safe">
        <item name="colorPrimary">@color/grayBorder</item>
        <item name="colorPrimaryDark">@color/grayBorder</item>
        <item name="colorAccent">@color/grayBorder</item>
        <item name="android:textColorHint">@color/text_color2</item>
        <item name="colorControlNormal">@color/grayBorder</item>
        <item name="colorControlActivated">@color/grayBorder</item>
    </style>

    <style name="Widget.MyTeam11.TextInputLayoutDisable" parent="AppTheme.Safe">
        <item name="colorAccent">@color/silver_two</item>
    </style>

    <style name="Widget.MyTeam11.RadioButton" parent="AppTheme.Safe">
        <item name="colorAccent">@color/maingreen</item>
        <item name="colorControlNormal">@color/grayBorder</item>
        <item name="colorControlActivated">@color/maingreen</item>
    </style>

    <style name="TextAppearance.MyTeam11.Large" parent="@android:style/TextAppearance.Large">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gunmetal</item>
    </style>

    <style name="TextAppearance.MyTeam11.Medium" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gunmetal</item>
    </style>

    <style name="TextAppearance.MyTeam11.Normal" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/gunmetal</item>
    </style>

    <style name="TextAppearance.MyTeam11.Small" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/cool_grey</item>
    </style>

    <style name="TextAppearance.MyTeam11.extraSmall" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/cool_grey</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="customProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal" />

    <style name="NavItemSelectedColor" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemTextColor">@color/maize</item>
    </style>

    <style name="SportTabStyle" parent="Widget.MaterialComponents.TabLayout">
        <item name="android:selectableItemBackground">@drawable/tab_sel</item>
    </style>

    <declare-styleable name="Const">
        <attr name="cl_theme" format="integer" />
    </declare-styleable>

    <style name="tab_text" parent="AppTheme.Safe">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="SpinnerwithNoArrow" parent="@android:style/Widget.Spinner" />

    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="MyTabLayoutTextAppearance.rubikMedium" parent="MyTabLayoutTextAppearance">
        <item name="android:textSize">12sp</item>
        <item name="fontFamily">@font/rubik_medium</item>
    </style>

    <style name="MyTabLayoutTextAppearanceLarge" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="MyTabLayoutTextAppearanceV6" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">11sp</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="MyTabLayoutTextAppearanceContest" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
        <item name="fontFamily">@font/rubik_medium</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="incorrect_text_TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="correct_text_TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="AppTheme.Regular.Small" parent="AppTheme">
        <item name="android:textSize">8sp</item>
        <item name="fontFamily">@font/rubik_medium</item>
    </style>

    <style name="NoDateMessage" parent="@android:style/TextAppearance">
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/cool_grey</item>
        <item name="fontFamily">@font/rubik_regular</item>
    </style>


    <style name="TextInputLayout" parent="@style/TextAppearance.AppCompat">
        <item name="android:textColorHint">@color/text_color2</item>
        <item name="android:textSize">@dimen/textSize10sp</item>
        <item name="android:fontFamily">@font/rubik_regular</item>
    </style>

    <style name="Widget.MyTeam11.TextInputLayoutActivate" parent="AppTheme.Safe">
        <item name="colorControlNormal">@color/text_color2</item>
        <item name="colorControlActivated">@color/text_color2</item>
        <item name="colorControlHighlight">@color/text_color2</item>
        <item name="android:colorFocusedHighlight">@color/text_color2</item>
        <item name="android:textColorHint">@color/text_color2</item>

    </style>

    <style name="BottomSheetDialog" parent="ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetModal</item>
    </style>

    <style name="BottomSheetModal" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>
</resources>
